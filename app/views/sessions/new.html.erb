<div class = "page">
<h1>Please Log In</h1>
<% if flash[:message] %>
  <%= flash[:message] %>
<% end%>
<%= form_tag login_path do %>
  <%= label_tag 'Username' %>
  <%= text_field_tag :username %>
  <br>
  <%= label_tag 'Password' %>
  <%= password_field_tag :password %>
  <br>
  <%= submit_tag "Log In" %>
<% end%>
</div>

